<library path="lib/libnav_utils_plugins">
    <class name="nav_utils/odom_to_tf"
           type="nav_utils::OdometryToTransformNodelet"
           base_class_type="nodelet::Nodelet">
        <description>
            Odometry to tf transform conversion.
        </description>
    </class>
    <class name="nav_utils/odom_proc"
           type="nav_utils::OdometryProcNodelet"
           base_class_type="nodelet::Nodelet">
        <description>
            Odometry message processing.
        </description>
    </class>
    <class name="nav_utils/tf_to_odom"
           type="nav_utils::TransformToOdometryNodelet"
           base_class_type="nodelet::Nodelet">
        <description>
            Tf Transform to odometry conversion.
        </description>
    </class>
    <class name="nav_utils/tf_fast_repeater"
           type="nav_utils::TfFastRepeaterNodelet"
           base_class_type="nodelet::Nodelet">
        <description>
            Repeat a TF on a higher frequency.
        </description>
    </class>
</library>
